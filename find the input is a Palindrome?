var str = 135
str  = String(str)
rev_str = str.split('').reverse().join('')
if (isPalin(str,rev_str)){
    console.log(str + " Is Palindrome")
}
else{
    console.log(str + " Is Not a Palindrome")
}

function isPalin(input,rev_input) {
    if (input == rev_input){
        return true
    }
    else {
        return false
    } 
}
